          <AssemblerScript>[enable]
alloc(nmbrosc_newmem,1024)

nmbrosc_newmem:
nmbrosc_code:
  push rbx
  push rdi
  push rsi
  sub rsp,20

  test rcx,rcx
  je short nmbrosc_return

  mov rdi,rcx
  movzx ebx,word ptr [rdi+06] //mist charges
  movzx esi,byte ptr [rdi+4E] //current mist bars

  call 00323A40 //modify mist charges

  mov [rdi+4E],sil
  mov [rdi+06],bx

nmbrosc_return:
  add rsp,20
  pop rsi
  pop rdi
  pop rbx
  ret


/* restores current mist bars and mist charges after the healall() vm call. */
define(nmbrosc_def,00331C58)
assert(nmbrosc_def,E8 E3 1D FF FF)
registersymbol(nmbrosc_def)

nmbrosc_def:
  call nmbrosc_code


[disable]
nmbrosc_def:
  db E8 E3 1D FF FF

unregistersymbol(nmbrosc_def)

dealloc(nmbrosc_newmem)

