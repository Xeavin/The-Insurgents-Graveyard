  cmp ecx,[r8+rax*4] //flag group
  je short lsr2_ignore

  dec eax
  jne short lsr2_loop

lsr2_check:
  jmp 003A1EA0

lsr2_ignore:
  mov eax,00000001
  ret


  align 10
lsr_fgl: //flag group list
  dd 06 //count

  //flag groups
  dd 04 //party menu - equip
  dd 06 //party menu - licenses
  dd 07 //party menu - clan primer
  dd 08 //party menu - party
  dd 09 //party menu - gambits
  dd 10 //party menu - world map


/* minimap. */
